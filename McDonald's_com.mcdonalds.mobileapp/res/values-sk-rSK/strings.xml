    <string name="apphome.account.verify_email.button_open_app.title">"Späť do McDonald 's aplikácie"</string>
    <string name="apphome.account.verify_email.success">"Tvoja e-mailová adresa bola úspešne overená! Ak máš záujem o newsletter McDonald's, nezabudni zapnúť tlačítko k odberu newslettru."</string>
    <string name="apphome.contact.header">Napíš nám</string>
    <string name="apphome.country.header">Vyber krajinu:</string>
    <string name="apphome.legal.header">"Vitaj v McDonald's"</string>
    <string name="apphome.legal.paragraph">"Naša aplikácia McDonald’s sa používa v rôznych krajinách a v každej tejto krajine platia iné podmienky jej používania. Nižšie nájdeš zoznam krajín, kde aplikácia funguje, a podmienky, ktoré v týchto krajinách platia. "</string>
