    <string name="apphome.account.button_delete.title">Smazat účet</string>
    <string name="apphome.account.delete_confirm">Tvoje žádost o zrušení účtu byla potvrzena. Vezmi prosím na vědomí, že rušení účtu může trvat i několik hodin.</string>
    <string name="apphome.account.dialog_confirm_delete.body">Pokud si smažeš účet, ztratíš přístup k některým kuponům a k dalším výhodám, které jsi během užívání získal. Tento krok nelze vzít zpět.</string>
